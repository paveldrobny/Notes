{
  "expo": {
    "name": "Мои заметки",
    "slug": "notes-mobile",
    "version": "1.3.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "notes-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alliancewd.notes"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "054c73f5-73c5-4373-bd2f-32356151c66f"
      }
    }
  }
}
